{
    "equation": {
        "prefix": "equation",
        "body": [
            "\\begin{equation}",
            "    $0",
            "    \\label{eq:$1}",
            "\\end{equation}"
        ],
        "description": "Insert equation env"
    },
    "item list": {
        "prefix": "itemize",
        "body": [
            "\\begin{itemize}",
            "    \\item $0",
            "\\end{itemize}"
        ],
        "description": "Insert item list"
    },
    "enumerated list": {
        "prefix": "itemize",
        "body": [
            "\\begin{enumerate}",
            "    \\item $0",
            "\\end{enumerate}"
        ],
        "description": "Insert enumerated list"
    },
    "figure with an image": {
        "prefix": "figimg",
        "body": [
            "\\begin{figure}",
            "    \\begin{center}",
            "        \\includegraphics[width=${1:0.9}\\textwidth]{${2:filename}}",
            "    \\end{center}",
            "    \\caption{$3}",
            "\\end{figure}",
            "$0"
        ],
        "description": "Insert a figure with an image"
    },
    "in-place listing": {
        "prefix": "lst",
        "body": [
            "\\begin{listing}[H]",
            "    \\begin{minted}{${1:cpp}}",
            "        $0",
            "    \\end{minted}",
            "    \\caption{$2}",
            "\\end{listing}"
        ],
        "description": "Insert an in-place listing"
    },
    "file listing": {
        "prefix": "lstfile",
        "body": [
            "\\begin{listing}[H]",
            "    \\inputminted{${1:cpp}}{${2:filename}}",
            "    \\caption{$3}",
            "\\end{listing}"
        ],
        "description": "Insert a listing from file"
    }
}
